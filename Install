(welcome)
(set @default-dest "SYS:")
(if (patmatch "68060" (database "cpu"))
	(set cpu_ver 1)
	(set cpu_ver 0)
)
(set cpu_ver (askchoice
	(prompt "Which CPU version to install?")
	(help @askchoice-help)
	(choices "68020+" "68060")
	(default cpu_ver)
))
(if (= cpu_ver 0)
	(set handlername "L/ssh2-handler.020")
	(set handlername "L/ssh2-handler.060")
)
(copylib
	(prompt "Copying ssh2-handler")
	(help @copylib-help)
	(source handlername)
	(dest "L:")
	(newname "ssh2-handler")
)
(exit)
